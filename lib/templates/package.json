{
  "force": {
    "version": "0.0.0-development",
    "scripts": {
      "test": "node gardener && nyc mocha --recursive",
      "coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
      "semantic-release": "semantic-release"
    }
  },
  "defaults": {
    "name": "???",
    "description": "???",
    "main": "lib/???.js",
    "keywords": [],
    "homepage": "{{GIT_URL}}#readme",
    "license": "MIT",
    "licenses": [
      {
        "type": "MIT",
        "url": "{{GIT_URL}}/blob/master/LICENSE"
      }
    ],
    "author": "???",
    "devDependencies": {
      "coveralls": "3.0.0",
      "nyc": "11.6.0",
      "semantic-release": "11.0.2"
    },
    "engines": {
      "node": ">= 6.10.3"
    },
    "repository": {
      "type": "git",
      "url": "{{GIT_URL}}.git"
    },
    "bugs": {
      "url": "{{GIT_URL}}/issues"
    },
    "files": [
      "lib"
    ],
    "nyc": {
      "tempDirectory": "coverage/.nyc_output",
      "check-coverage": true,
      "per-file": false,
      "lines": 100,
      "statements": 100,
      "functions": 100,
      "branches": 100,
      "include": [
        "**/*.js"
      ],
      "exclude": [
        "gardener.js",
        "node_modules/*",
        "coverage/*",
        "flow-typed/*"
      ],
      "reporter": [
        "lcov",
        "text-summary"
      ],
      "require": [
        "babel-register"
      ],
      "extension": [],
      "cache": true,
      "all": true,
      "report-dir": "./coverage"
    }
  }
}
