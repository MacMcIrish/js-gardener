{
  "force": {
    "version": "0.0.0-development",
    "scripts": {
      "test": "grunt gardener",
      "coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
      "semantic-release": "semantic-release pre && npm publish && semantic-release post"
    }
  },
  "defaults": {
    "homepage": "{{GIT_URL}}",
    "license": "MIT",
    "licenses": [
      {
        "type": "MIT",
        "url": "{{GIT_URL}}/blob/master/LICENSE"
      }
    ],
    "devDependencies": {
      "coveralls": "3.0.0",
      "semantic-release": "8.2.0",
      "cracks": "3.1.2"
    },
    "engines": {
      "node": ">= 6.10.3"
    },
    "repository": {
      "type": "git",
      "url": "{{GIT_URL}}.git"
    },
    "bugs": {
      "url": "{{GIT_URL}}/issues"
    },
    "release": {
      "verifyRelease": {
        "path": "cracks",
        "paths": ["test", "package.json", "package-lock.json"],
        "silent": true
      }
    }
  }
}
