{
  "name": "js-gardener",
  "version": "0.0.0-development",
  "description": "Minimizes project setup and maintenance complexity, so you can focus on writing code.",
  "main": "lib/gardener.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">= 8.10"
  },
  "scripts": {
    "build": "npx babel src --out-dir lib --source-maps inline --copy-files --include-dotfiles",
    "test-simple": "nyc mocha \"./test/**/*.spec.js\"",
    "test": "npm run clean && npm run gardener && npm run test-simple",
    "coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "semantic-release": "npm run build-clean && npx semantic-release",
    "clean": "rm -rf lib",
    "build-clean": "npm run clean && npm run build",
    "gardener": "npm run build-clean && node gardener && npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blackflux/js-gardener.git"
  },
  "keywords": [
    "test",
    "automation",
    "best practises",
    "guide",
    "npm",
    "helper",
    "setup",
    "ci"
  ],
  "author": "Lukas Siemon",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/blackflux/js-gardener/blob/master/LICENSE"
    }
  ],
  "bugs": {
    "url": "https://github.com/blackflux/js-gardener/issues"
  },
  "homepage": "https://github.com/blackflux/js-gardener#readme",
  "dependencies": {
    "chalk": "2.4.2",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-flowtype": "3.5.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jasmine": "2.10.1",
    "eslint-plugin-json": "1.4.0",
    "eslint-plugin-markdown": "1.0.0",
    "eslint-plugin-mocha": "5.3.0",
    "fancy-log": "1.3.3",
    "flow-bin": "0.96.0",
    "fs-extra": "7.0.1",
    "glob": "7.1.3",
    "js-yaml": "3.13.1",
    "lodash.defaults": "4.2.0",
    "lodash.difference": "4.5.0",
    "lodash.get": "4.4.2",
    "mocha": "6.1.1",
    "npm-check": "5.9.0",
    "object-scan": "5.0.3",
    "parse-github-url": "1.0.2",
    "robo-config": "1.20.0",
    "tmp": "0.1.0",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@babel/plugin-transform-flow-comments": "7.4.0",
    "@babel/preset-flow": "7.0.0",
    "@babel/register": "7.4.0",
    "@blackflux/robo-config-plugin": "1.1.0",
    "babel-eslint": "10.0.1",
    "chai": "4.2.0",
    "coveralls": "3.0.3",
    "nyc": "13.3.0",
    "semantic-release": "15.13.3"
  },
  "peerDependencies": {},
  "nyc": {
    "tempDir": "./coverage/.nyc_output",
    "report-dir": "./coverage",
    "check-coverage": true,
    "per-file": false,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "gardener.js",
      "node_modules/*",
      "coverage/*",
      "flow-typed/*",
      "lib/*",
      "src/conf/rules/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "@babel/register"
    ],
    "extension": [],
    "cache": true,
    "all": true,
    "babel": true
  }
}
