{
  "analyzeCommits": {
    "preset": "angular",
    "releaseRules": [
      {
        "type": "build",
        "scope": "deps",
        "release": "patch"
      }
    ]
  },
  "npmPublish": false,
  "publish": [
    {
      "path": "@semantic-release/github",
      "assets": [
